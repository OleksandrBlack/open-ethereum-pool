<h4>Recently Found Blocks:</h4>
{{#if model.candidates}}
    <table class="table table-condensed table-striped">
        <thead>
        <tr>
            <th>Height</th>
            <th>Time Found</th>
            <th>Variance</th>
        </tr>
        </thead>
        <tbody>
        {{#each model.candidates as |block|}}
            <tr>
                <td><a href="{{format-explorer 'block' block.height}}" rel="nofollow" target="_blank">{{format-number block.height}}</a></td>
                <td>{{format-date-locale block.timestamp}}</td>
                <td>
                    {{#if block.isLucky}}
                        <h5 class="m--font-success m--font-boldest"><strong>{{format-number block.variance style='percent'}}</strong></h5>
                    {{else}}
                        <h5 class="">{{format-number block.variance style='percent'}}</h5>
                    {{/if}}
                </td>
            </tr>
        {{/each}}
        </tbody>
    </table>
{{else}}
    <h3>-</h3>
{{/if}}
<h4>Immature Blocks:</h4>
{{#if model.immature}}
    <table class="table table-condensed table-striped">
        <thead>
        <tr>
            <th>Height</th>
            <th>Time Found</th>
            <th>Variance</th>
            <th>Reward</th>
            <th>Block Hash</th>
        </tr>
        </thead>
        <tbody>
        {{#each model.immature as |block|}}
            {{partial "blocks/block"}}
        {{/each}}
        </tbody>
    </table>
{{else}}
    <h3>-</h3>
{{/if}}
<h4>Matured Blocks:</h4>
{{#if model.matured}}
    <table class="table table-condensed table-striped">
        <thead>
        <tr>
            <th>Height</th>
            <th>Time Found</th>
            <th>Variance</th>
            <th>Reward</th>
            <th>Block Hash</th>
        </tr>
        </thead>
        <tbody>
        {{#each model.matured as |block|}}
            {{partial "blocks/block"}}
        {{/each}}
        </tbody>
    </table>
{{else}}
    <h3>-</h3>
{{/if}}
