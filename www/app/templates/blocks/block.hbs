<tr>
  <td>
    {{#if block.uncle}}
      <a href="{{format-explorer 'block' block.uncleHeight}}" rel="nofollow" target="_blank">{{format-number block.height}}</a>
    {{else}}
      <a href="{{format-explorer 'block' block.height}}" rel="nofollow" target="_blank">{{format-number block.height}}</a>
    {{/if}}
  </td>
  <td>{{format-date-locale block.timestamp}}</td>
  <td>
    {{#if block.isLucky}}
      <h5 class="m--font-success m--font-boldest"><strong>{{format-number block.variance style='percent'}}</strong></h5>
    {{else}}
      <h5 class="">{{format-number block.variance style='percent'}}</h5>
    {{/if}}
  </td>
  <td>
    {{#if block.uncle}}
    <h5 class="m--font-metal">{{block.formatReward}}</h5>
    {{else if block.isOk}}
    <h5 class="m--font-primary">{{block.formatReward}}</h5>
    {{/if}}
  </td>
    <td>
        {{#if block.uncle}}
            <a href="{{format-explorer 'block' block.uncleHeight}}" rel="nofollow" target="_blank">{{block.hash}}</a>
        {{else if block.orphan}}
            <span class="label label-danger m--font-danger">Orphan</span>
        {{else}}
            <a href="{{format-explorer 'block' block.height}}" rel="nofollow" target="_blank">{{block.hash}}</a>
        {{/if}}
    </td>
</tr>
